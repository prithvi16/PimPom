<div id="app">
   

    <div class="row">
        <div class="col s12">

            <div id="newsub">

                <div class="entername">

                    <div class="introtext">
                        Pimpom allows you to create an awesome poll. Share it with your friends and know what they think about you!
                    </div>
                    
                            <div class="card z-depth-5">
                                <div class="card-image">
                                    <img src="https://i.imgur.com/yGtRIKu.jpg">
                
                                </div>
                
                                
                            </div>
                       
                    <div class="introtext">
                        Enter the name by which your friends call you
                        <input style="max-width:30rem;" placeholder="Bancho" type="text" v-model="qname">
                    </div>

                    <div class="centerbutton">
                        <button @click="submitNewQuiz" class="purple lighten-1 btn waves-effect waves-light" type="submit" name="action">Create Poll!

                        </button>
                    </div>


                </div>



            </div>

            <div id="thelink" class="hide-my-ass">
                <div class="row">

                    <div class="col s12">
                        <div class="introtext">
                            This is your link. Take a screenshot or Share it!
                        </div>
                        <div class="introtext">

                            <a :href=qlink>

                                {{"pimpom.ml" + qlink}}
                            </a>
                        </div>


                    </div>

                </div>

                <div class="sharing">


                    <!-- Sharingbutton Facebook -->
                    <a class="resp-sharing-button__link" :href=fblink target="_blank" aria-label="Share on Facebook">
                        <div class="resp-sharing-button resp-sharing-button--facebook resp-sharing-button--large">
                            <div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solidcircle">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                                    <path d="M12 0C5.38 0 0 5.38 0 12s5.38 12 12 12 12-5.38 12-12S18.62 0 12 0zm3.6 11.5h-2.1v7h-3v-7h-2v-2h2V8.34c0-1.1.35-2.82 2.65-2.82h2.35v2.3h-1.4c-.25 0-.6.13-.6.66V9.5h2.34l-.24 2z"
                                    />
                                </svg>
                            </div>Share on Facebook</div>
                    </a>

                    <!-- Sharingbutton Twitter -->
                    <a class="resp-sharing-button__link" :href=twlink target="_blank" aria-label="Share on Twitter">
                        <div class="resp-sharing-button resp-sharing-button--twitter resp-sharing-button--large">
                            <div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solidcircle">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
                                    <path d="M12 0C5.38 0 0 5.38 0 12s5.38 12 12 12 12-5.38 12-12S18.62 0 12 0zm5.26 9.38v.34c0 3.48-2.64 7.5-7.48 7.5-1.48 0-2.87-.44-4.03-1.2 1.37.17 2.77-.2 3.9-1.08-1.16-.02-2.13-.78-2.46-1.83.38.1.8.07 1.17-.03-1.2-.24-2.1-1.3-2.1-2.58v-.05c.35.2.75.32 1.18.33-.7-.47-1.17-1.28-1.17-2.2 0-.47.13-.92.36-1.3C7.94 8.85 9.88 9.9 12.06 10c-.04-.2-.06-.4-.06-.6 0-1.46 1.18-2.63 2.63-2.63.76 0 1.44.3 1.92.82.6-.12 1.95-.27 1.95-.27-.35.53-.72 1.66-1.24 2.04z"
                                    />
                                </svg>
                            </div>Share on Twitter</div>
                    </a>

                    <!-- Sharingbutton Google+ -->
                    <a class="resp-sharing-button__link" :href=golink target="_blank" aria-label="Share on Google+">
                        <div class="resp-sharing-button resp-sharing-button--google resp-sharing-button--large">
                            <div aria-hidden="true" class="resp-sharing-button__icon resp-sharing-button__icon--solidcircle">
                                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24">
